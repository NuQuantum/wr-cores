   CONFIG.ADDR_WIDTH {32} \
   CONFIG.DATA_WIDTH {32} \
   CONFIG.HAS_REGION {0} \
   CONFIG.NUM_READ_OUTSTANDING {8} \
   CONFIG.NUM_WRITE_OUTSTANDING {8} \
   CONFIG.PROTOCOL {AXI3} \
   ] $wr_axi


  # Create ports
  set RTIO_AXI_aclk [ create_bd_port -dir O -type clk RTIO_AXI_aclk ]
  set_property -dict [ list \
   CONFIG.ASSOCIATED_BUSIF {RTIO_AXI} \
   CONFIG.ASSOCIATED_RESET {RTIO_AXI_resetn} \
 ] $RTIO_AXI_aclk
  set RTIO_AXI_resetn [ create_bd_port -dir O -from 0 -to 0 -type rst RTIO_AXI_resetn ]
